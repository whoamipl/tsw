extends layout
block content
    h1.m-1 Twoje przedmioty
    div.container.mt-5
        each item in items
            li.list-group-item
                div.row
                    div.col
                        img.img-thumbnail(src='http://localhost:3000/photos/' + item.img.filename)
                    div.col.text-center.mt-auto.mb-auto=item.title

                